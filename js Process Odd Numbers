function score(){
    let arg = Array.from(arguments);
    let arr = arg[0];
    let result = [];

    for (let index = 0; index < arr.length; index++) {
        
        if(index % 2 !== 0){
            result.push(+arr[index]* 2);
        }
    }
    
    result.reverse();
    console.log(result.join(' '));
    

}

score(['10','15','20','25']);
