function solve(arr,arr2){
    let num = arr;
    let num2 = arr2;
    let divisor = 0;
    
    for (let index = 2; index <= 9; index++) {
        if(arr % index === 0 && arr2 % index === 0){
            divisor = index;
        }
    
        if(divisor > 0){
            break;
        }
    }
    console.log(divisor);
    
    }
    solve(15,5);
